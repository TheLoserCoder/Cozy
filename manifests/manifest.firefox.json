{
  "manifest_version": 3,

  "browser_specific_settings": {
    "gecko": {
      "id": "newtab@yourdomain.com",
      "strict_min_version": "109.0"
    }
  },

  "background": {
    "scripts": ["worker.js"],
    "type": "module"
  },

  "permissions": [
    "theme"
  ],

  "host_permissions": [
    "<all_urls>",
    "*://*/*"
  ]
}
